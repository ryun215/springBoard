<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="member.mapper">
	
	<!-- Insert -->
	<insert id="memberInsert" parameterType="com.naver.cafe.member.service.MemberDto">
		insert into member(
		m_id, 
		m_pw, 
		m_name, 
		m_level
		) 
		values(
		#{mId}, 
		#{mPw}, 
		#{mName}, 
		#{mLevel}
		)
	</insert>
</mapper>


