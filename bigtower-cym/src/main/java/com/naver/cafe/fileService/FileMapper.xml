<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="file.mapper">
	
	<!-- Insert -->
	<insert id="fileUpload" parameterType="com.naver.cafe.fileService.FileDto">
		INSERT INTO file(
			f_name,
			f_path
		)
		VALUES(
			#{fName},
			#{fPath}
		)
	</insert>
</mapper>